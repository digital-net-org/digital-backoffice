    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug: Backoffice",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "runtimeArgs": ["run-script", "dev:backoffice"],
            "runtimeExecutable": "npm",
            "skipFiles": ["<node_internals>/**"],
            "type": "node"
        },
        {
            "name": "Debug: UI",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "runtimeArgs": ["run-script", "dev:ui"],
            "runtimeExecutable": "npm",
            "skipFiles": ["<node_internals>/**"],
            "type": "node"
        }
    ]
}
